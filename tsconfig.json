{
  "compilerOptions": {
    "outDir": "./dist",
    "allowJs": true,
    "target": "es5",
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "lib": ["ESNext"],
    "downlevelIteration": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "alwaysStrict": true
    // "noUnusedLocals": true,
    // "noImplicitAny": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", "generated"]
}
